{
    "name": "agentsoftware/laravel-raw-sqs-connector",
    "description": "Allows for the consumption of raw messages produced outside of Laravel from SQS to then be handled natively within Laravel's Queue and Job system",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Harnasz",
            "email": "tom@primitivesense.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "aws/aws-sdk-php": "^3.2",
        "illuminate/cache": "^10.48",
        "illuminate/queue": "^8 || ^9 || ^10"
    },
    "autoload": {
        "psr-4": {
            "AgentSoftware\\LaravelRawSqsConnector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13",
        "mockery/mockery": "^1.2",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "lint": "php-cs-fixer fix",
        "test:lint": "php-cs-fixer fix --dry-run",
        "test:unit": "phpunit",
        "test:types": "phpstan",
        "test": [
            "@test:unit",
            "@test:lint",
            "@test:types"
        ]
    }
}
